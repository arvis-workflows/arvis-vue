{
  "$schema": "https://raw.githubusercontent.com/jopemachine/arvis-extension-validator/master/workflow-schema.json",
  "category": "Uncategorised",
  "creator": "Radimir Bitsov",
  "name": "arvis-vue",
  "readme": "",
  "enabled": true,
  "version": "0.0.5",
  "description": "Search for Vue.js API references on vuejs.org",
  "webAddress": "https://github.com/jopemachine/arvis-vue",
  "defaultIcon": "icon.png",
  "commands": [
    {
      "type": "scriptFilter",
      "command": "vue",
      "title": "Vue.js API",
      "subtitle": "",
      "scriptFilter": "node index.js \"{query}\"",
      "runningSubtext": "Searching...",
      "withspace": true,
      "argType": "required",
      "actions": [
        {
          "modifiers": "normal",
          "type": "open",
          "target": "{query}"
        }
      ]
    }
  ]
}
